@using LocalizedStrings = Resources.CommonResources;
<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/themes/base/jquery-ui.css")" rel="stylesheet" type="text/css"/>
    <script src="@Url.Content("~/Scripts/jquery-1.9.0.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/knockout-2.2.1.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/ko.custom.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/ListGeneratorViewModel.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.10.0.min.js")" type="text/javascript"></script>
</head>
<body>
    <div class="page">
        <div id="header">
            <div id="title" class="title-block-line">
                <div id="logo">
                    <img  src="/Content/Images/fls_logo.png" alt="First Line Software"/>
                </div>
                <div align="right">
                    <h1 align="right">@LocalizedStrings.MainHeader</h1>
                </div>
            </div>
        </div>
        <div id="main">
            @RenderBody()
        </div>
    </div>
</body>
</html>
